### Model Card Template - Metadata

# How to use this YAML file:
# Fill in your information within the quotations ("") e.g. model_display_name: "ModelX"  
# ~ or ~
# Add a info block for each item that you would like to add to a category 
# e.g. authors:  
#         - name: "Jane Doe"
#           type: "individual"
#           affiliate: "Chan Zuckerberg Initiative"  
#         - name: "Chan Zuckerberg Initiative"
#           type: "organization"
# ~ or ~
# Choose from a list provided by deleting the irrelevant items leaving only the list items you wish to select
# You can also add your own items to these lists by adding a list item with a dash
# e.g. tasks_performed_by_model:       
#        - Cell Clustering
#        - Cell Labeling
#        - Cross Species Integration
#.       - Special Task Added To List

### Basic Info
model_display_name: "{{ model_display_name }}"
model_version: "{{ model_version }}"                       # vX.X.X or YYYY-MM-DD format
primary_contact_email: "{{ primary_contact_email }}"
repository_link: "{{ repository_link }}"
publication_preprint_link: "{{ publication_preprint_link }}"
release_date: "{{ release_date }}"                        # YYYY-MM-DD
model_domain: "{{ model_domain }}"                        # one of "Imaging" or "Transcriptomic" or "Text"
model_description: "{{ model_description }}"
short_description: "{{ short_description }}"                 # Max 160 characters
{%- if authors %}
authors:                        # List one or more individuals or organizations 
{%- for author in authors %}
  - name: "{{ author.name }}"
    type: "{{ author.type }}"                    #   one of "individual" or "organization"
    affiliate: "{{ author.affiliate }}"                   #   (optional) if it is an "individual", add an affiliated organization name
{%- endfor -%}
{%- else %}
authors:                        # List one or more individuals or organizations 
  - name: ""
    type: ""                    #   one of "individual" or "organization"
    affiliate: ""                                   #   (optional) if it is an "individual", add an affiliated organization name
{%- endif -%}
{%- if licenses %}
licenses:                       # Add one or more licenses
{%- for license in licenses %}
  - type: "{{ license.type }}"                    #   e.g "CC BY 4.0"
    url: "{{ license.url }}"                     #   e.g "https://creativecommons.org/licenses/by/4.0/deed.en"
{%- endfor -%}
{%- else %}
licenses:                       # Add one or more licenses
  - type: "{{ license_name }}"                    #   e.g "CC BY 4.0"
    url: ""                     #   e.g "https://creativecommons.org/licenses/by/4.0/deed.en"
{%- endif %}
compute_requirement: "{{ compute_requirement }}"             # one of "GPU" or "CPU"
system_requirements: "{{ system_requirements }}"         # (optional) Any other system constraints or requirements you would like to describe.

### Model Details
{%- if model_architecture_type %}
model_architecture_type:        # Choose from any architectures or packages below and/or add onto the list
{%- for arch in model_architecture_type %}
  - {{ arch }}
{%- endfor -%}
{%- else %}
model_architecture_type:        # Choose from any architectures or packages below and/or add onto the list
  - PyTorch
  - TensorFlow
  - JAX
  - Safetensors
  - Transformers
  - PEFT
  - TensorBoard
  - GGUF
  - Diffusers
  - ONNX
  - stable-baselines3
  - sentence-transformers
  - ml-agents
  - MLX
  - TF-Keras
  - Adapters
  - Keras
  - setfit
  - timm
  - sample-factory
  - Transformers.js
{%- endif -%}
{%- if tasks_performed_by_model %}
tasks_performed_by_model:       # Choose from any tasks below and/or add onto the list
{%- for task in tasks_performed_by_model %}
  - {{ task }}
{%- endfor -%}
{%- else %}
tasks_performed_by_model:       # Choose from any tasks below and/or add onto the list
  - Batch Correction
  - Cell Clustering
  - Cell Labeling
  - Cell Morphology Profiling
  - Cell Type Annotation
  - Cell Type Classification
  - Contrast Transfer Function (CTF) Estimation
  - Cross Species Integration
  - CryoET Particle Picking
  - Data Integration
  - Differential Expression
  - Feature Extraction
  - Frame Alignment & Motion Correction
  - Gene Co-expression Prediction
  - Gene Network Inference
  - Hypothesis Generation
  - Image Segmentation
  - Imputation
  - Perturbation Detection/Prediction
  - Synthetic Data Generation
  - Protein Localization
  - Tomogram Alignment / Reconstruction
  - Virtual Staining
{%- endif -%}
{%- if finetuned_from %}
finetuned_from:                 # (optional) If applicable. Add one or more.
{%- for model in finetuned_from %}
  - model_name: "{{ model.model_name }}"
    model_url: "{{ model.model_url }}"
{%- endfor -%}
{%- else %}
finetuned_from:                 # (optional) If applicable. Add one or more.
  - model_name: ""
    model_url: ""
{%- endif -%}
{%- if model_variants %}
model_variants:                 # (optional) If applicable. Add one or more.
{%- for variant in model_variants %}
  - variant_name: "{{ variant.variant_name }}"
    variant_description: "{{ variant.variant_description }}"
    variant_url: "{{ variant.variant_url }}"
{%- endfor -%}
{%- else %}
model_variants:                 # (optional) If applicable. Add one or more.
  - variant_name: ""
    variant_description: ""
    variant_url: ""
{%- endif -%}

### Training Details
training_date: "{{ training_date }}"                         # (optional) YYYY-MM-DD
uses_synthetic_data: "{{ uses_synthetic_data }}"           # one of "Yes" or "No". Was synthetic data used in developing the model?
uses_purchased_licensed_data: "{{ uses_purchased_licensed_data }}"  # one of "Yes" or "No". Were any datasets used for model training purchased or licensed? 
{%- if input_data_type %}
input_data_type:                # Choose from any data types below and/or add onto the list
{%- for data_type in input_data_type %}
  - {{ data_type }}
{%- endfor -%}
{%- else %}
input_data_type:                # Choose from any data types below and/or add onto the list
  - pdf
  - svg
  - png
  - jpg
  - zarr
  - czi
  - tiff
  - mrc
  - hdf5
  - csv
  - docx
  - txt
{%- endif -%}
{%- if output_data_type %}
output_data_type:               # Choose from any data types below and/or add onto the list
{%- for data_type in output_data_type %}
  - {{ data_type }}
{%- endfor -%}
{%- else %}
output_data_type:               # Choose from any data types below and/or add onto the list
  - embeddings
  - cell types
  - images
{%- endif -%}
# To gather Training Dataset data, we use the ~Cross Modality Schema~ which standardizes biological information. 
# https://github.com/chanzuckerberg/data-guidance/blob/main/standards/cross-modality/1.1.0/schema.md
{%- if training_datasets %}

training_datasets:
{%- for dataset in training_datasets %}
  - dataset_name: "{{ dataset.dataset_name }}"
    dataset_licenses: 
{%- if dataset.dataset_licenses %}
{%- for license in dataset.dataset_licenses %}
      - type: "{{ license.type }}"                    #   e.g "CC BY 4.0"
        url: "{{ license.url }}"                     #   e.g "https://creativecommons.org/licenses/by/4.0/deed.en"
{%- endfor -%}
{%- else %}
      - type: ""                    #   e.g "CC BY 4.0"
        url: ""                     #   e.g "https://creativecommons.org/licenses/by/4.0/deed.en"
{%- endif %}
    dataset_version: "{{ dataset.dataset_version }}"
    dataset_url: "{{ dataset.dataset_url }}"
    dataset_date_published: "{{ dataset.dataset_date_published }}"               # YYYY-MM-DD
    dataset_collection_timeframe: "{{ dataset.dataset_collection_timeframe }}"         # YYYY-MM/YYYY-MM
    dataset_first_used_in_model_dev: "{{ dataset.dataset_first_used_in_model_dev }}"      # YYYY-MM When was the dataset first used during model development?
    includes_consumer_info: "{{ dataset.includes_consumer_info }}"               # one of "Yes" or "No". Does the dataset include any aggregate consumer information?
    modality: "{{ dataset.modality }}"                             # one of "Imaging" or "Transcriptomic" or "Text"
    # If modality = Transcriptomic:
    assay_ontology_term_id: "{{ dataset.assay_ontology_term_id }}"               # e.g. "EFO:0009922" Search the Experimental Factor Ontology (https://www.ebi.ac.uk/ols4/ontologies/efo/) for dataset assay like "10x 3' v3" or "Perturb-Seq" and copy the id.
    # If modality = Imaging
    {%- if dataset.imaging_technologies %}
    imaging_technologies:                    # Choose imaging technology types from the list below and/or write in a different technology
    {%- for tech in dataset.imaging_technologies %}
      - {{ tech }}
    {%- endfor %}
    {%- else %}
    imaging_technologies:                    # Choose imaging technology types from the list below and/or write in a different technology
      - Brightfield Microscopy
      - Phase Contrast Microscopy
      - Differential Interference Contrast (DIC)
      - Fluorescence Microscopy
      - Confocal Microscopy
      - Spinning Disk Confocal Microscopy
      - Two-Photon Microscopy
      - Total Internal Reflection Fluorescence (TIRF)
      - STED
      - SIM
      - PALM
      - STORM
      - Transmission Electron Microscopy (TEM)
      - Scanning Electron Microscopy (SEM)
      - Cryo-Electron Microscopy (Cryo-EM)
      - Cryo-Electron Tomography (Cryo-ET)
      - Imaging Mass Cytometry (IMC)
      - Multiplexed Ion Beam Imaging (MIBI)
      - CODEX
      - Cyclic Immunofluorescence (CycIF)
      - Multiplexed Immunohistochemistry (mIHC)
      - seqFISH / MERFISH
      - Hematoxylin & Eosin (H&E) Staining
      - Immunohistochemistry (IHC)
      - Immunofluorescence (IF)
      - Whole Slide Imaging (WSI)
      - Electron microscopy
      - Cryo-electron microscopy
      - Fluorescence light microscopy
      - Transmission light microscopy
      - Super resolution microscopy
      - X-ray microscopy
      - Force microscopy
      - High-contrast microscopy
      - Whole-slide microscopy
    {%- endif %}
    disease_ontology_term_id: "{{ dataset.disease_ontology_term_id }}"             # e.g "PATO:0000461" for normal or healthy. Please follow these instructions for diseased and injured: https://github.com/chanzuckerberg/data-guidance/blob/main/standards/cross-modality/1.1.0/schema.md#disease_ontology_term_id
    organism_ontology_term_id: "{{ dataset.organism_ontology_term_id }}"            # e.g. "NCBITaxon:9606" for Human. Please follow these instructions: https://github.com/chanzuckerberg/data-guidance/blob/main/standards/cross-modality/1.1.0/schema.md#organism_ontology_term_id
    development_stage_ontology_term_id: "{{ dataset.development_stage_ontology_term_id }}"   # Please follow these instructions: https://github.com/chanzuckerberg/data-guidance/blob/main/standards/cross-modality/1.1.0/schema.md#development_stage_ontology_term_id
    tissue_type: "{{ dataset.tissue_type }}"                          # one of "tissue", "organoid", "cell culture", "cell line" or "organelle"
    tissue_ontology_term_id: "{{ dataset.tissue_ontology_term_id }}"              # Please follow these instructions: https://github.com/chanzuckerberg/data-guidance/blob/main/standards/cross-modality/1.1.0/schema.md#tissue_ontology_term_id
    num_of_cells: "{{ dataset.num_of_cells }}"                         # Enter an approximate number e.g. 1000000
{%- endfor -%}
{%- else %}

training_datasets:
  - dataset_name: ""
    dataset_licenses: 
      - type: ""                    #   e.g "CC BY 4.0"
        url: ""                     #   e.g "https://creativecommons.org/licenses/by/4.0/deed.en"
